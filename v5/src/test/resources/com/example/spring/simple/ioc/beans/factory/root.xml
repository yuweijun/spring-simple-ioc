<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<!--
		Just included for the count: not to mean anything in particular
	-->
	<bean id="something" class="com.example.spring.simple.ioc.beans.factory.support.DependenciesBean"/>

	<bean id="indexedBean" class="com.example.spring.simple.ioc.beans.IndexedTestBean"/>

	<!-- Overridden by next factory -->
	<bean id="test" class="com.example.spring.simple.ioc.beans.TestBean">
		<property name="name"><value>custom</value></property>
		<property name="age"><value>25</value></property>
	</bean>

	<bean id="testFactory1" class="com.example.spring.simple.ioc.beans.factory.DummyFactory"/>

	<bean id="testFactory2" class="com.example.spring.simple.ioc.beans.factory.DummyFactory">
		<property name="singleton"><value>false</value></property>
	</bean>

</beans>
